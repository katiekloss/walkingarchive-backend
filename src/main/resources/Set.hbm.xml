<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="org.walkingarchive.backend.model.card.Set" table="Sets">
        <id name="id" type="java.lang.Integer" column="setid">
            <generator class="native" />
        </id>

        <property name="name">
            <column name="setname" />
        </property>
    </class>
</hibernate-mapping>